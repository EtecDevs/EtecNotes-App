{
  "expo": {
    "name": "EtecNotes",
    "slug": "etecnotes-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#8C43FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.etecdevs.etecnotes"
    },
    "android": {
      "package": "com.etecdevs.etecnotes",
      "adaptiveIcon": {
        "backgroundColor": "#8C43FF"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
